{
	"env": {
		"browser": true
	},
	"extends": ["plugin:react/recommended", "react-app"],
	"plugins": ["react"],
	"rules": {
		"camelcase": 1,
		"no-unused-vars": 2,
		"eqeqeq": 2,
		"no-console": [2, { "allow": ["warn", "error"] }],
		"no-case-declarations": 0,
		"indent": [2, "tab", { "SwitchCase": 1 }],
		"quotes": [2, "single", "avoid-escape"],
		"semi": 2,
		"curly": 2,
		"brace-style": 2,
		"react/display-name": 0,
		"react/no-unescaped-entities": 0,
		"react/prop-types": 0,
		"react/jsx-no-bind": 2,
		"react/no-deprecated": 1
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	}
}
